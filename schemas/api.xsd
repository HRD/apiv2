<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<schema targetNamespace="http://api.hrd.pl/api/"
        elementFormDefault="unqualified" xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:hrd="http://api.hrd.pl/api/"
        xmlns:Q1="http://api.hrd.pl/api/">
    <element name="api" type="hrd:apiType"></element>

    <group name="apiTypes">
        <choice>
            <element name="token" type="hrd:tokenType"></element>
            <element name="login" type="hrd:loginType"></element>
            <element name="poll" type="hrd:pollType"></element>
            <element name="action" type="hrd:actionType"></element>
            <element name="partner" type="hrd:partnerType"></element>
            <element name="news" type="hrd:newsType"></element>
            <element name="user" type="hrd:userType"></element>
            <element name="domain" type="hrd:domainType"></element>
            <element name="cert" type="hrd:certType"></element>
        </choice>
    </group>

    <group name="pollTypes">
        <choice>
            <element name="get" type="hrd:emptyTagType"></element>
            <element name="ack" type="hrd:pollAckType"></element>
        </choice>
    </group>

    <group name="actionTypes">
        <choice>
            <element name="info" type="hrd:actionIdType"></element>
            <element name="list" type="hrd:actionListType"></element>
        </choice>
    </group>

    <group name="partnerTypes">
        <choice>
            <element name="getBalance" type="hrd:emptyTagType"></element>
            <element name="getPricings" type="hrd:emptyTagType"></element>
            <element name="getPricingsList" type="hrd:emptyTagType"></element>
            <element name="getPricingInfo" type="hrd:pricingInfoType"></element>
            <element name="resendTradeMail" type="hrd:partnerResendTradeMailType"></element>
            <element name="getKeys" type="hrd:emptyTagType"></element>
            <element name="setKey" type="hrd:partnerSetKeyType"></element>
            <element name="contactCreate" type="hrd:contactCreateType"></element>
            <element name="contactUpdate" type="hrd:contactUpdateType"></element>
            <element name="contactDelete" type="hrd:contactIdType"></element>
            <element name="contactInfo" type="hrd:contactIdType"></element>
            <element name="contactList" type="hrd:contactListType"></element>
        </choice>
    </group>

    <group name="newsTypes">
        <choice>
            <element name="info" type="hrd:newsInfoType"></element>
            <element name="list" type="hrd:newsListType"></element>
        </choice>
    </group>

    <group name="userTypes">
        <choice>
            <element name="info" type="hrd:userIdType"></element>
            <element name="create" type="hrd:userCreateType"></element>
            <element name="update" type="hrd:userUpdateType"></element>
            <element name="list" type="hrd:userListType"></element>
        </choice>
    </group>

    <group name="domainTypes">
        <choice>
            <element name="authInfoCheck" type="hrd:domainAuthInfoCheckType"></element>
            <element name="check" type="hrd:domainCheck">
                <unique name="domainNameUnique">
                    <selector xpath="./name"></selector>
                    <field xpath="."></field>
                </unique>
            </element>
            <element name="info" type="hrd:domainNameType"></element>
            <element name="book" type="hrd:domainBookType"></element>
            <element name="create" type="hrd:domainCreateType"></element>
            <element name="transfer" type="hrd:domainTransferType"></element>
            <element name="transferStatus" type="hrd:actionIdType"></element>
            <element name="transferRegistrantEmail" type="hrd:transferRegistrantEmailType"></element>
            <element name="trade" type="hrd:domainTradeType"></element>
            <element name="tradeAssignorCancel" type="hrd:actionConfirmCancelType"></element>
            <element name="tradeAssigneeCancel" type="hrd:actionConfirmCancelType"></element>
            <element name="tradeCancel" type="hrd:domainTradeCancelType"></element>
            <element name="renew" type="hrd:domainRenewType"></element>
            <element name="update" type="hrd:domainUpdateType"></element>
            <element name="list" type="hrd:domainListType"></element>
            <element name="privacySet" type="hrd:domainPrivacySetType"></element>
            <element name="nsGroupInfo" type="hrd:domainNsGroupInfoType"></element>
            <element name="nsGroupCreate" type="hrd:domainNsGroupCreateType">
                <unique name="nsGroupCreateUniqueName">
                    <selector xpath="./ns/name"></selector>
                    <field xpath="."></field>
                </unique>
            </element>
            <element name="nsGroupUpdate" type="hrd:domainNsGroupUpdateType">
                <unique name="nsGroupUpdateUniqueName">
                    <selector xpath="./ns/name"></selector>
                    <field xpath="."></field>
                </unique>
            </element>
            <element name="nsGroupDelete" type="hrd:domainNsGroupInfoType"></element>
            <element name="nsGroupList" type="hrd:domainNsGroupListType"></element>
            <element name="whois" type="hrd:domainNameType"></element>
            <element name="contactList" type="hrd:domainNameType"></element>
            <element name="contactSet" type="hrd:domainContactSetType"></element>
            <element name="tradeGetPw" type="hrd:domainNameType"></element>
            <element name="tradeInnerGetPw" type="hrd:domainNameType"></element>
            <element name="tradeStatus" type="hrd:actionIdType"></element>
            <element name="tradePayerConfirm" type="hrd:actionIdType"></element>
            <element name="futureCheck" type="hrd:domainCheck">
                <unique name="domainFutureNameUnique">
                    <selector xpath="./name"></selector>
                    <field xpath="."></field>
                </unique>
            </element>
            <element name="futureCreate" type="hrd:domainFutureCreateType"></element>
            <element name="futureList" type="hrd:domainListType"></element>
            <element name="futureInfo" type="hrd:domainNameType"></element>
            <element name="futureRenew" type="hrd:domainFutureRenewType"></element>
            <element name="dnsInfo" type="hrd:domainNameType"></element>
            <element name="dnsChange" type="hrd:domainDnsChangeType"></element>
            <element name="dnssecAdd" type="hrd:domainDnsSecType"></element>
            <element name="dnssecDelete" type="hrd:domainDnsSecType"></element>
            <element name="hostCreate" type="hrd:domainHostCreateType"></element>
            <element name="hostDelete" type="hrd:domainNameType"></element>
            <element name="hostUpdate" type="hrd:domainHostCreateType"></element>
            <element name="hostInfo" type="hrd:domainNameType"></element>
            <element name="hostList" type="hrd:domainListType"></element>
        </choice>
    </group>

    <group name="certTypes">
        <choice>
            <element name="info" type="hrd:certInfoType"></element>
            <element name="create" type="hrd:certCreateType"></element>
            <element name="renew" type="hrd:certRenewType"></element>
            <element name="list" type="hrd:certListType"></element>
            <element name="get" type="hrd:certInfoType"></element>
            <element name="contactInfo" type="hrd:certContactInfoType"></element>
            <element name="contactList" type="hrd:certContactListType"></element>
        </choice>
    </group>

    <complexType name="apiType">
        <sequence>
            <choice>
                <group ref="hrd:apiTypes" />
            </choice>
            <element name="id" minOccurs="0" maxOccurs="1" type="hrd:idType"></element>
        </sequence>
    </complexType>

    <simpleType name="idType">
        <restriction base="string">
            <minLength value="1"></minLength>
            <maxLength value="255"></maxLength>
            <pattern value="[A-Za-z0-9]+"></pattern>
        </restriction>
    </simpleType>

    <simpleType name="dateType">
        <restriction base="string">
            <pattern value="\d{4}-\d{2}-\d{2}"></pattern>
        </restriction>
    </simpleType>

    <complexType name="emptyTagType"></complexType>

    <simpleType name="domainNameSimpleType">
        <restriction base="string">
            <minLength value="3"></minLength>
            <maxLength value="255"></maxLength>
            <pattern
                    value="(([a-z0-9]{1,63})|([a-z0-9][a-z0-9_\-]{0,61}[a-z0-9]))(\.(([a-z0-9]{1,63})|([a-z0-9][a-z0-9_\-]{0,61}[a-z0-9])))*\.(([a-z0-9]{1,63})|([a-z0-9][a-z0-9_\-]{0,61}[a-z0-9]))">
            </pattern>
        </restriction>
    </simpleType>

    <simpleType name="domainDnsNameSimpleType"> <!-- same as domainNameSimpleType but with "." at end -->
        <restriction base="string">
            <minLength value="4"></minLength>
            <maxLength value="255"></maxLength>
            <pattern
                    value="(([a-z0-9]{1,63})|([a-z0-9][a-z0-9_\-]{0,61}[a-z0-9]))(\.(([a-z0-9]{1,63})|([a-z0-9][a-z0-9_\-]{0,61}[a-z0-9])))*\.(([a-z0-9]{1,63})|([a-z0-9][a-z0-9_\-]{0,61}[a-z0-9]))\.">
            </pattern>
        </restriction>
    </simpleType>

    <complexType name="domainDnsSecType">
        <sequence>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="alg" type="unsignedInt"></element>
            <element name="digestType" type="unsignedInt"></element>
            <element name="digest" type="hrd:dnssecDigestType"></element>
            <choice>
                <element name="keyTag" type="unsignedInt"></element>
                <element name="keyType" type="hrd:dnssecKeyType"></element>
            </choice>
        </sequence>
    </complexType>

    <simpleType name="dnssecDigestType">
        <restriction base="string">
            <minLength value="40"></minLength>
            <maxLength value="4096"></maxLength>
        </restriction>
    </simpleType>

    <simpleType name="dnssecKeyType">
        <restriction base="string">
            <enumeration value="zsk"></enumeration>
            <enumeration value="ksk"></enumeration>
        </restriction>
    </simpleType>

    <complexType name="periodType">
        <simpleContent>
            <extension base="unsignedShort">
                <attribute name="unit" default="y">
                    <simpleType>
                        <restriction base="string">
                            <enumeration value="y"></enumeration>
                            <enumeration value="m"></enumeration>
                            <enumeration value="d"></enumeration>
                        </restriction>
                    </simpleType>
                </attribute>
            </extension>
        </simpleContent>
    </complexType>

    <complexType name="nsOrGroupType">
        <choice>
            <element name="group" type="unsignedInt"></element>
            <element name="ns">
                <complexType>
                    <sequence>
                        <element name="ns" minOccurs="2" maxOccurs="16">
                            <complexType>
                                <sequence>
                                    <element name="name" type="hrd:domainNameSimpleType"></element>
                                    <choice minOccurs="0" maxOccurs="10">
                                        <element name="ipv4" type="hrd:ipv4Type"></element>
                                        <element name="ipv6" type="hrd:ipv6Type"></element>
                                    </choice>
                                </sequence>
                            </complexType>
                            <unique name="nsIpUnique">
                                <selector xpath="./ipv4 | ./ipv6"></selector>
                                <field xpath="."></field>
                            </unique>
                        </element>
                    </sequence>
                </complexType>
                <unique name="nsNameUnique">
                    <selector xpath="./ns/name"></selector>
                    <field xpath="."></field>
                </unique>
            </element>
        </choice>
    </complexType>

    <simpleType name="ipv4Type">
        <restriction base="string">
            <minLength value="7"></minLength>
            <maxLength value="15"></maxLength>
            <pattern
                    value="((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)">
            </pattern>
        </restriction>
    </simpleType>

    <simpleType name="ipv6Type">
        <restriction base="string">
            <pattern
                    value="([0-9A-Fa-f]{1,4}:([0-9A-Fa-f]{1,4}:([0-9A-Fa-f]{1,4}:([0-9A-Fa-f]{1,4}:([0-9A-Fa-f]{1,4}:[0-9A-Fa-f]{0,4}|:[0-9A-Fa-f]{1,4})?|(:[0-9A-Fa-f]{1,4}){0,2})|(:[0-9A-Fa-f]{1,4}){0,3})|(:[0-9A-Fa-f]{1,4}){0,4})|:(:[0-9A-Fa-f]{1,4}){0,5})((:[0-9A-Fa-f]{1,4}){2}|:(25[0-5]|(2[0-4]|1[0-9]|[1-9])?[0-9])(\.(25[0-5]|(2[0-4]|1[0-9]|[1-9])?[0-9])){3})|(([0-9A-Fa-f]{1,4}:){1,6}|:):[0-9A-Fa-f]{0,4}|([0-9A-Fa-f]{1,4}:){7}:">
            </pattern>
        </restriction>
    </simpleType>

    <complexType name="domainSE_NUAdditionalDataType">
        <choice>
            <sequence>
                <element name="individual" type="hrd:emptyTagType"></element>
                <element name="id" type="string"></element>
            </sequence>
            <sequence>
                <element name="organization" type="hrd:emptyTagType"></element>
                <element name="vat" type="string"></element>
                <element name="registrationNumber" type="string"></element>
            </sequence>
        </choice>
    </complexType>

    <complexType name="domainROAdditionalDataType">
        <sequence>
            <element name="id" type="string"></element>
        </sequence>
    </complexType>

    <complexType name="domainPM_RE_TF_WF_YTAdditionalDataType">
        <choice>
            <sequence>
                <element name="individual" type="hrd:emptyTagType"></element>
                <element name="countryOfBirth" type="hrd:countryType"></element>
                <element name="dateOfBirth" type="hrd:dateType"></element>
                <element name="cityOfBirth" type="string"></element>
                <element name="postalCodeOfBirth" type="string"></element>
                <element name="provinceOfBirth" type="string"></element>
            </sequence>
            <sequence>
                <element name="organization" type="hrd:emptyTagType"></element>
                <element name="vat" type="string" minOccurs="0"></element>
                <element name="registrationNumber" type="string" minOccurs="0"></element>
                <element name="trademarkNumber" type="string" minOccurs="0"></element>
            </sequence>
        </choice>
    </complexType>

    <complexType name="domainLVAdditionalDataType">
        <choice>
            <sequence>
                <element name="individual" type="hrd:emptyTagType"></element>
                <element name="id" type="string"></element>
            </sequence>
            <sequence>
                <element name="organization" type="hrd:emptyTagType"></element>
                <element name="registrationNumber" type="string"></element>
                <element name="vat" type="string"></element>
            </sequence>
        </choice>
    </complexType>

    <complexType name="domainHUAdditionalDataType">
        <choice>
            <sequence>
                <element name="individual" type="hrd:emptyTagType"></element>
                <element name="id" type="string"></element>
            </sequence>
            <sequence>
                <element name="organization" type="hrd:emptyTagType"></element>
                <element name="vat" type="string"></element>
            </sequence>
        </choice>
    </complexType>

    <complexType name="domainIndividualOrOrganizationAdditionalDataType">
        <sequence>
            <element name="type">
                <simpleType>
                    <restriction base="string">
                        <enumeration value="individual"></enumeration>
                        <enumeration value="organization"></enumeration>
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>

    <complexType name="domainAdditionalDataType">
        <choice>
            <element name="fr">
                <complexType>
                    <sequence>
                        <element name="registrantType">
                            <simpleType>
                                <restriction base="string">
                                    <enumeration value="individual"></enumeration>
                                    <enumeration value="organization"></enumeration>
                                </restriction>
                            </simpleType>
                        </element>
                    </sequence>
                </complexType>
            </element>
            <element name="it">
                <complexType>
                    <sequence>
                        <element name="entityType">
                            <simpleType>
                                <restriction base="string">
                                    <enumeration value="1"></enumeration>
                                    <enumeration value="2"></enumeration>
                                    <enumeration value="3"></enumeration>
                                    <enumeration value="4"></enumeration>
                                    <enumeration value="5"></enumeration>
                                    <enumeration value="6"></enumeration>
                                    <enumeration value="7"></enumeration>
                                </restriction>
                            </simpleType>
                        </element>
                        <element name="regCode" type="string"></element>
                    </sequence>
                </complexType>
            </element>
            <element name="us">
                <complexType>
                    <sequence>
                        <element name="category">
                            <simpleType>
                                <restriction base="string">
                                    <enumeration value="C11"></enumeration>
                                    <enumeration value="C12"></enumeration>
                                    <enumeration value="C21"></enumeration>
                                    <enumeration value="C31"></enumeration>
                                    <enumeration value="C32"></enumeration>
                                </restriction>
                            </simpleType>
                        </element>
                        <element name="purpose">
                            <simpleType>
                                <restriction base="string">
                                    <enumeration value="P1"></enumeration>
                                    <enumeration value="P2"></enumeration>
                                    <enumeration value="P3"></enumeration>
                                    <enumeration value="P4"></enumeration>
                                    <enumeration value="P5"></enumeration>
                                </restriction>
                            </simpleType>
                        </element>
                    </sequence>
                </complexType>
            </element>
            <element name="ca">
                <complexType>
                    <sequence>
                        <element name="type">
                            <simpleType>
                                <restriction base="string">
                                    <enumeration value="CCT"></enumeration>
                                    <enumeration value="RES"></enumeration>
                                    <enumeration value="ABO"></enumeration>
                                    <enumeration value="LGR"></enumeration>
                                    <enumeration value="CCO"></enumeration>
                                    <enumeration value="GOV"></enumeration>
                                    <enumeration value="EDU"></enumeration>
                                    <enumeration value="ASS"></enumeration>
                                    <enumeration value="HOP"></enumeration>
                                    <enumeration value="PRT"></enumeration>
                                    <enumeration value="TDM"></enumeration>
                                    <enumeration value="TRD"></enumeration>
                                    <enumeration value="PLT"></enumeration>
                                    <enumeration value="LAM"></enumeration>
                                    <enumeration value="TRS"></enumeration>
                                    <enumeration value="INB"></enumeration>
                                    <enumeration value="OMK"></enumeration>
                                    <enumeration value="MAJ"></enumeration>
                                </restriction>
                            </simpleType>
                        </element>
                    </sequence>
                </complexType>
            </element>
            <element name="es">
                <complexType>
                    <sequence>
                        <element name="legalForm">
                            <simpleType>
                                <restriction base="string">
                                    <enumeration value="1"></enumeration>
                                    <enumeration value="39"></enumeration>
                                    <enumeration value="47"></enumeration>
                                    <enumeration value="59"></enumeration>
                                    <enumeration value="68"></enumeration>
                                    <enumeration value="124"></enumeration>
                                    <enumeration value="150"></enumeration>
                                    <enumeration value="152"></enumeration>
                                    <enumeration value="164"></enumeration>
                                    <enumeration value="181"></enumeration>
                                    <enumeration value="197"></enumeration>
                                    <enumeration value="203"></enumeration>
                                    <enumeration value="229"></enumeration>
                                    <enumeration value="269"></enumeration>
                                    <enumeration value="286"></enumeration>
                                    <enumeration value="365"></enumeration>
                                    <enumeration value="434"></enumeration>
                                    <enumeration value="436"></enumeration>
                                    <enumeration value="439"></enumeration>
                                    <enumeration value="476"></enumeration>
                                    <enumeration value="510"></enumeration>
                                    <enumeration value="524"></enumeration>
                                    <enumeration value="525"></enumeration>
                                    <enumeration value="554"></enumeration>
                                    <enumeration value="560"></enumeration>
                                    <enumeration value="562"></enumeration>
                                    <enumeration value="566"></enumeration>
                                    <enumeration value="608"></enumeration>
                                    <enumeration value="612"></enumeration>
                                    <enumeration value="713"></enumeration>
                                    <enumeration value="717"></enumeration>
                                    <enumeration value="744"></enumeration>
                                    <enumeration value="746"></enumeration>
                                    <enumeration value="747"></enumeration>
                                    <enumeration value="877"></enumeration>
                                    <enumeration value="878"></enumeration>
                                    <enumeration value="879"></enumeration>
                                </restriction>
                            </simpleType>
                        </element>
                        <element name="documentId" type="string"></element>
                    </sequence>
                </complexType>
            </element>
        </choice>
    </complexType>

    <complexType name="tokenType">
        <sequence>
            <element name="token">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"></minLength>
                        <maxLength value="255"></maxLength>
                        <pattern value="[A-Za-z0-9\+/=]+"></pattern>
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>

    <complexType name="loginType">
        <sequence>
            <element name="login">
                <simpleType>
                    <restriction base="string">
                        <maxLength value="32"></maxLength>
                        <minLength value="1"></minLength>
                    </restriction>
                </simpleType>
            </element>
            <element name="pass" type="hrd:String1_255Type"></element>
            <element name="type">
                <simpleType>
                    <restriction base="string">
                        <enumeration value="partnerApi"></enumeration>
                        <enumeration value="partner"></enumeration>
                        <enumeration value="special"></enumeration>
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>

    <complexType name="domainType">
        <choice>
            <group ref="hrd:domainTypes" />
        </choice>
    </complexType>

    <complexType name="domainCheck">
        <sequence>
            <element name="name" minOccurs="1" maxOccurs="100"
                     type="hrd:domainNameSimpleType">
            </element>
        </sequence>
    </complexType>

    <complexType name="domainFutureCreateType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="user" type="unsignedInt"></element>
        </all>
    </complexType>

    <complexType name="domainFutureRenewType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="currentExpirationDate" type="hrd:dateType"></element>
        </all>
    </complexType>

    <complexType name="actionIdType">
        <all>
            <element name="id" type="unsignedInt"></element>
        </all>
    </complexType>

    <complexType name="domainNameType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
        </all>
    </complexType>

    <complexType name="domainCreateType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="user" type="unsignedInt"></element>
            <element name="period" type="hrd:periodType"></element>
            <element name="privacyProtect" type="hrd:boolean"></element>
            <element name="ns" type="hrd:nsOrGroupType"></element>
            <element name="additionalData" type="hrd:domainAdditionalDataType"
                     minOccurs="0">
            </element>
        </all>
    </complexType>

    <complexType name="domainTransferType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="user" type="unsignedInt"></element>
            <element name="period" type="hrd:periodType" minOccurs="0"></element>
            <element name="pw" type="string"></element>
        </all>
    </complexType>

    <complexType name="transferRegistrantEmailType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="pw" type="string"></element>
        </all>
    </complexType>

    <complexType name="domainTradeType">
        <sequence>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="user" type="unsignedInt"></element>
            <element name="pw" type="string"></element>
        </sequence>
    </complexType>

    <complexType name="domainRenewType">
        <sequence>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="currentExpirationDate" type="hrd:dateType"></element>
            <element name="period" type="hrd:periodType"></element>
        </sequence>
    </complexType>

    <complexType name="domainUpdateType">
        <sequence>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="ns" type="hrd:nsOrGroupType" minOccurs="0"></element>
        </sequence>
    </complexType>

    <complexType name="domainListType">
        <sequence>
            <element name="lastName" type="hrd:domainNameSimpleType"
                     minOccurs="0">
            </element>
        </sequence>
    </complexType>

    <complexType name="domainPrivacySetType">
        <all>
            <element name="name" type="hrd:domainNameSimpleType"></element>
            <element name="privacy" type="hrd:boolean"></element>
        </all>
    </complexType>

